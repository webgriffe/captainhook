{
    "name": "webgriffe/captainhook",
    "description": "Provides some useful custom actions to be used with Captain Hook (a PHP GIT hooks library)",
    "type": "library",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-posix": "*",
        "captainhook/captainhook": "^5.0",
        "sebastianfeldmann/git": "^3.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Webgriffe\\CaptainHook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Webgriffe\\CaptainHook\\": "spec/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Webgriffe SRL",
            "email": "support@webgriffe.com"
        }
    ]
}
